ScriptButton.Activated:Connect(function()
	local toHide = {
		MiscTab,
		InfoTab,
		NewScriptTab
	}
	ScriptTab.Visible = not ScriptTab.Visible

	for _, e in ipairs(toHide) do
		if e.Visible == true then
			e.Visible = false
		end
	end
end)

MiscButton.Activated:Connect(function()
	local toHide = {
		ScriptTab,
		InfoTab,
		NewScriptTab
	}
	MiscTab.Visible = not MiscTab.Visible

