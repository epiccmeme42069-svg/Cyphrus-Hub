local OUiCorner = Instance.new("UICorner")
OUiCorner.CornerRadius = UDim.new(0,8)
OUiCorner.Archivable = true
OUiCorner.Parent = Open_Close_Button

Open_Close_Button.Activated:Connect(function()
	Cyphrus.Enabled = not Cyphrus.Enabled
	
	if Cyphrus.Enabled == true then
		Open_Close_Button.Text = "Close"
	else
		Open_Close_Button.Text = "Open"
	end
end)

