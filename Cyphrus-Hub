FPS.Activated:Connect(function()
	game.ReplicatedFirst.TourneyQ:Destroy()
	game.Workspace.Terrain.WaterWaveSize = 0
	game.Workspace.Terrain.WaterWaveSpeed = 0
	game.Workspace.Terrain.WaterReflectance = 0
	game.Lighting.Brightness = 0
	game.Workspace.Terrain.WaterTransparency = 0
	game.Lighting.GlobalShadows = false
	settings().Rendering.QualityLevel = "Level01"
	Plr.PlayerGui.HUD:Destroy()

	for e, f in pairs(game:GetDescendants()) do
		if f:IsA("Part") or f:IsA("Union") or f:IsA("CornerWedgePart") or f:IsA("TrussPart") then
			f.Reflectance = 0
		elseif f:IsA("Decal") or f:IsA("Texture") or f:IsA("ForceField") then
			f:Destroy()
		elseif f:IsA("ParticleEmitter") or f:IsA("Trail") then
			f:Destroy()
		elseif f:IsA("Explosion") then
			f.BlastPressure = 0
			f.BlastRadius = 0
		elseif f:IsA("Fire") or f:IsA("SpotLight") or f:IsA("Smoke") or f:IsA("Sparkles") then
			f:Destroy()
		end
	end
	
	for e, g in pairs(game.Lighting:GetChildren()) do
		if
			g:IsA("BlurEffect") or g:IsA("SunRaysEffect") or g:IsA("ColorCorrectionEffect") or g:IsA("BloomEffect") or
			g:IsA("DepthOfFieldEffect")
		then
			g:Destroy()
		end
	end
end)

