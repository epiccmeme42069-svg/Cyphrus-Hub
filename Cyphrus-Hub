local UIS = game:GetService("UserInputService")
local Frame = Main
local dragToggle = nil
local dragSpeed = .10
local dragStart = nil
local startPos = nil

local function updateInput(input)
	local delta = input.Position - dragStart
	local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
		startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	game:GetService("TweenService"):Create(Frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
end

Frame.InputBegan:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
		dragToggle = true
		dragStart = input.Position
		startPos = Frame.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragToggle = false
			end
		end)
	end
end)

UIS.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		if dragToggle then
			updateInput(input)
		end
	end
end)

local function getCharacter()
	return Plr.Character or Plr.CharacterAdded:Wait()
end

local function updateCount()
	local count = 0

	for _, tool in ipairs(Backpack:GetChildren()) do
		if tool:IsA("Tool") and tool.Name == "Double Weight" then
			count += 1
		end
	end

	local character = getCharacter()
	for _, tool in ipairs(character:GetChildren()) do
		if tool:IsA("Tool") and tool.Name == "Double Weight" then
			count += 1
		end
	end

	DWCounter.Text = "Double Weight(s): " .. count
end

local function connectEvent()
	local character = getCharacter()

	Backpack.ChildAdded:Connect(updateCount)
	Backpack.ChildRemoved:Connect(updateCount)

	character.ChildAdded:Connect(updateCount)
	character.ChildRemoved:Connect(updateCount)
end

Plr.CharacterAdded:Connect(function()
	wait(.1)
	connectEvent()
	updateCount()
end)

connectEvent()
updateCount()


local LeaderStr = Plr.leaderstats.Strength
local curvalue = LeaderStr.Value
local Gained = 0
LeaderStr:GetPropertyChangedSignal("Value"):Connect(function()
	local delta = (LeaderStr.Value - curvalue);
	Gained += delta;
	StrGain.Text = "Strength Gained: "..Gained;
	curvalue = LeaderStr.Value;
end)

local RealStr = game.ReplicatedStorage.Data[Plr.Name].Strength

RealStr:GetPropertyChangedSignal("Value"):Connect(function()
	Str.Text = "Strength: "..RealStr.Value
end)

